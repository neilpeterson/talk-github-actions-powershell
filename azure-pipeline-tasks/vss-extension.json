{
	"manifestVersion": 1,
	"id": "pipeline-utilities",
	"version": "1.0.3",
	"name": "Pipeline Utilities",
	"description": "A PowerShell demo Azure Pipeline extension.",
	"publisher": "nepeters",
	"categories": [
		"Azure Pipelines"
	],
    "content": {
        "details": {
            "path": "docs/overview.md"
        }
    },
	"public": true,
	"galleryFlags": [
        "Preview"
    ],
	"targets": [{
		"id": "Microsoft.VisualStudio.Services"
	}],
	"icons": {
		"default": "icon.png"
	},
	"contributions": [{
			"id": "count",
			"type": "ms.vss-distributed-task.task",
			"targets": [
				"ms.vss-distributed-task.tasks"
			],
			"properties": {
				"name": "count"
			}
		}
	],
	"files": [{
			"path": "count",
			"addressable": true
		}
	]
}